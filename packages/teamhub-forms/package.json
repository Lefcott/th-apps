{
  "name": "teamhub-forms",
  "scripts": {
    "start": "webpack-dev-server --https --port 9999",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext js",
    "format": "prettier --write \"./**\"",
