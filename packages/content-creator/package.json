{
	"name": "content-creator",
	"version": "0.0.1",
	"description": "Content Editor for K4 Community",
	"main": "index.js",
	"scripts": {
		"test": "jasmine",
		"start": "K4_ENV=local webpack-dev-server --config webpack.config.js --config-name=\"@teamhub/content-creator\"",
		"start:ci": "K4_ENV=local webpack-dev-server --config webpack.config.js --config-name=\"@teamhub/content-creator\" --port 8081 --https & npx wait-on https-get://localhost:8081",
		"build": "webpack --config webpack.config.js",
		"cy-dev": "cypress open",
		"cy-run": "cypress run",
		"docs:dev": "documentation serve --watch cypress/**",
		"docs:md": "documentation build cypress/** -f md --markdown-toc-max-depth 2 -o cypress/README.md",
		"docs": "documentation build cypress/** -f html -o docs && open docs/index.html"
	},
