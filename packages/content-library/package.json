{
  "name": "content-library",
  "version": "1.0.0",
  "description": "Content library teamhub module",
  "main": "index.js",
  "scripts": {
    "test": "cross-env BABEL_ENV=test jest",
    "test-watch": "BABEL_ENV=test jest --watch",
    "start": "webpack-dev-server --https",
    "start:ci": "webpack-dev-server --port 8081 --https & npx wait-on https-get://localhost:8081",
    "build": "webpack --mode=production",
    "cy-dev": "cypress open",
    "cy-run": "cypress run --browser chrome",
    "cy:ci": "cypress run --record --parallel",
    "docs:dev": "documentation serve --watch cypress/**",
    "docs:md": "documentation build cypress/** -f md --markdown-toc-max-depth 2 -o cypress/README.md",
    "docs": "documentation build cypress/** -f html -o docs && open docs/index.html",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext js --fix",
    "format": "prettier --write \"./**\"",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k4connect/content-library.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/k4connect/content-library/issues"
  },
  "homepage": "https://github.com/k4connect/content-library#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "prettier --write",
      "eslint --max-warnings 10 --fix"
    ],
    "*.{md,json,html,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.8.7",
    "@k4connect/teamhub-apollo-config": "0.0.9",
    "@testing-library/dom": "^7.21.6",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^12.0.17",
    "@types/jest": "^25.2.1",
    "@types/systemjs": "^6.1.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "cypress-file-upload": "^5.0.3",
    "documentation": "^12.1.4",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-important-stuff": "^2.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^6.0.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "jest-canvas-mock": "^2.2.0",
    "jest-cli": "^25.2.7",
    "jest-extended": "^0.11.5",
    "lint-staged": "^9.5.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.12.0",
    "single-spa-react": "^2.14.0",
    "start-server-and-test": "^1.12.1",
    "systemjs-webpack-interop": "^1.1.2",
    "waait": "^1.0.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-config-single-spa-react": "^1.0.3",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "core-js": "^3.6.4",
    "date-fns": "^2.10.0",
    "formik": "^2.1.3",
    "fs": "0.0.1-security",
    "lodash": "^4.17.15",
    "luxon": "^1.24.1",
    "material-ui-dropzone": "^3.5.0",
    "notistack": "^0.9.9",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-content-loader": "^5.0.1",
    "react-dom": "^17.0.2",
    "react-lottie": "^1.2.3",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.25.2",
    "slick-carousel": "^1.8.1",
    "subscriptions-transport-ws": "^0.9.16",
    "whatwg-fetch": "^2.0.4",
    "yup": "^0.28.1"
  }
}
