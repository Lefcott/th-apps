    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.1",
    "@fortawesome/pro-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.2.10",
    "awesome-phonenumber": "^2.20.0",
    "babel-plugin-import": "^1.13.0",
    "core-js": "^3.2.1",
    "element-scroll-polyfill": "^1.0.1",
    "formik": "^2.2.9",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "luxon": "^1.25.0",
    "notistack": "^0.9.9",
    "prop-types": "^15.7.2",
    "rc-swipeout": "^2.0.11",
    "react": "^16.8.0",
    "react-content-loader": "^4.2.2",
    "react-dom": "^16.8.0",
    "react-input-mask": "^2.0.4",
    "react-select-material-ui": "^4.3.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-virtuoso": "^0.11.1",
    "use-debounce": "^3.4.2",
    "webpack-merge": "^5.0.9",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "webpack-dev-server --port 3007 --https",
    "start:ci": "webpack-dev-server --port 3007 --https & npx wait-on https-get://localhost:3007",
    "start-mock": "PORT=3007 REACT_APP_MOCK=true react-scripts start",
    "build": "webpack --mode production",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "test-no-watch": "CI=true react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "cy-dev": "cypress open",
    "cy:ci": "cypress run --record --parallel",
    "docs:dev": "documentation serve --watch cypress/**",
    "docs:md": "documentation build cypress/** -f md --markdown-toc-max-depth 2 -o cypress/README.md",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext js",
    "format": "prettier --write './**'",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "pre-push": "CI=true npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "npm run test",
      "git add ."
    ]
  },
  "prettier": "@k4connect/caregiver-prettier",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.8.7",
    "@k4connect/caregiver-prettier": "^1.0.1",
    "@k4connect/teamhub-apollo-config": "0.0.9",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.2.1",
    "@types/systemjs": "^6.1.0",
    "apollo-link-schema": "^1.2.3",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "concurrently": "^5.0.1",
    "cross-env": "^7.0.2",
    "cypress-file-upload": "^5.0.8",
    "documentation": "^12.1.4",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-important-stuff": "^2.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^6.0.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "jest-cli": "^25.2.7",
    "lint-staged": "^12.3.1",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "single-spa-react": "^2.14.0",
    "systemjs-webpack-interop": "^1.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-config-single-spa-react": "^1.0.3",
    "webpack-dev-server": "^3.11.0"
  }
}
