{
  "name": "teamhub-auth",
  "scripts": {
    "start": "webpack-dev-server --port 3010 --https",
    "start:ci": "K4_ENV=local webpack-dev-server --config webpack.config.js --config-name=\"@teamhub/auth\" --port 3010 --https & npx wait-on https-get://localhost:3010",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env.analyze=true",
    "lint": "eslint src --ext js --fix",
    "format": "prettier --write './**'",
    "test": "cross-env BABEL_ENV=test K4_ENV=test jest --passWithNoTests",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "cy-dev": "cypress open",
    "cy-dev:local": "npm run start:ci && cypress open --env ENV=local",
    "cy-docs": "documentation build cypress/** -f md --markdown-toc-max-depth 2 -o cypress/README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "prettier": "@k4connect/caregiver-prettier",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-optional-chaining": "^7.12.0",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.8.7",
    "@k4connect/caregiver-prettier": "^1.0.1",
    "@testing-library/dom": "^7.26.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.7",
    "@types/jest": "^25.2.1",
    "@types/systemjs": "^6.1.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.0.1",
    "cross-env": "^7.0.2",
    "documentation": "^12.1.4",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-important-stuff": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^6.0.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.3",
    "jest-cli": "^26.5.3",
    "jsonwebtoken": "^8.5.1",
    "msw": "^0.21.2",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "single-spa-react": "^2.14.0",
    "systemjs-webpack-interop": "^1.1.2",
    "url-search-params-polyfill": "^8.1.0",
    "waait": "^1.0.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-config-single-spa-react": "^1.0.3",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.4.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "formik": "^2.1.4",
    "lodash.escaperegexp": "^4.1.2",
    "notistack": "^1.0.1",
    "raygun4js": "^2.20.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "yup": "^0.29.1"
  }
}
