{
  "name": "staff-directory",
  "scripts": {
    "start": "webpack-dev-server --port 3008 --https",
    "start:ci": "webpack-dev-server --port 3008 --https & npx wait-on https-get://localhost:3008",
    "start-mock": "PORT=3007 REACT_APP_MOCK=true react-scripts start",
    "build": "webpack --mode production",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests",
    "lint": "eslint src --ext js",
    "format": "prettier --write './**'",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "cy-dev": "cypress open",
    "cy:ci": "cypress run --record --parallel",
    "cy-docs": "documentation build cypress/** -f md --markdown-toc-max-depth 2 -o cypress/README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently npm:test npm:lint"
    }
  },
  "prettier": "@k4connect/caregiver-prettier",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.8.7",
    "@k4connect/caregiver-prettier": "^1.0.1",
    "@k4connect/teamhub-apollo-config": "0.0.10",
    "@testing-library/dom": "^7.23.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.1.3",
    "@types/jest": "^25.2.1",
    "@types/systemjs": "^6.1.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "concurrently": "^5.0.1",
    "cross-env": "^7.0.2",
    "cypress-file-upload": "^5.0.4",
    "documentation": "^13.0.2",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-important-stuff": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "flush-promises": "^1.0.2",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.7",
    "jest-cli": "^25.2.7",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "single-spa-react": "^2.14.0",
    "systemjs-webpack-interop": "^1.1.2",
    "uuid": "^3.4.0",
    "waait": "^1.0.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-config-single-spa-react": "^1.0.3",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^5.1.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@k4connect/caregiver-components": "^0.4.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "awesome-phonenumber": "^2.36.0",
    "element-scroll-polyfill": "^1.0.1",
    "formik": "^2.1.5",
    "google-libphonenumber": "^3.2.13",
