    "react-image-crop": "^8.6.6",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.6.2",
    "react-text-mask": "^5.4.3",
    "react-use": "^15.3.3",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.6",
    "yup": "^0.29.3"
  }
